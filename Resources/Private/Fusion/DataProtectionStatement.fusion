prototype(LegalWeb.GdprTools:DataProtectionStatement) < prototype(Neos.Neos:ContentComponent) {
    renderer = afx`
        <div>{LegalWeb.GdprTools.Services.getDataProtectionStatement()}</div>
    `

    @cache {
        mode = 'uncached'
        context {
            1 = 'node'
            2 = 'documentNode'
        }
    }
}
